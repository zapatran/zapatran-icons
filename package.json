  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.12",
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.4",
    "semantic-release": "^19.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "scripts": {
    "generate:outline": "npx @svgr/cli --typescript -d src/components/outline src/svg/outline",
    "generate:solid": "npx @svgr/cli --typescript -d src/components/solid src/svg/solid",
    "generate": "npm run generate:outline && npm run generate:solid",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "format": "prettier --write  \"src/**/*.{js,ts,jsx,tsx,json}\"",
    "commit": "git-cz",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 6006",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release"
  }
}
